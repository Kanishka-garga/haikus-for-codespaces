#include<iostream>
using namespace std;
class Rectangle{
  private:
      int length,breadth;
  public:
      Rectangle(int l=0, int b=0);
      void setLength(int l);
      void setBreadth(int b);
           int getLength()
           {
               return length;
           }
           int getBreadth()
           {
               return breadth;
           }
      };
      Rectangle::Rectangle(int l,int b)
      {
          length=l;
          breadth=b;
      }
      void Rectangle::setLength(int l)
      {
          if(l<=0)
          {
              cout<<"invalid value entered"<<endl;
          }
          else
          {
              length=l;
          }
      }
      void Rectangle::setBreadth(int b)
          {
              if(b<=0)
              {
                  cout<<"invalid value entered"<<endl;
              }
              else{
                breadth=b;
              }
           }

class cuboid:public Rectangle{
   private:
       int height;
   public:
    cuboid(int h=0)
    {
        height=h;
    }
    void setHeight(int h)
    {
        if(h<=0)
        {
            cout<<"invalid value of height";
        }
        else{
            height=h;
        }
    }
    int getHeight()
    {
        return height;
    }
    //getlength();
    //getBreadth();//
        int volume()
      {
         return getLength()* getBreadth()* getHeight();
      }
    void display()
       {

        cout<<"the volume is "<<volume()<<endl;
       }


};
int main()
{

   cuboid c(-3);
   c.setLength(5);
   c.setBreadth(4);
   c.display();

}
